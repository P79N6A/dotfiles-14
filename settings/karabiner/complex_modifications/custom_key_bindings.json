    {
      "description": "COMMAND + Backslash to toggle iTerm.app",
      "manipulators": [
        {
          "from": {
            "key_code": "backslash",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'iTerm'"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.googlecode\\.iterm2$"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "backslash",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
