{
  "title": "Complex Modifications Rules",
  "rules": [
    {
      "description": "Toggle fn_locked if hold down Fn key",
      "manipulators": [
        {
          "from": {
            "simultaneous": [
              {
                "key_code": "fn"
              },
              {
                "key_code": "left_control"
              },
              {
                "key_code": "left_option"
              },
              {
                "key_code": "left_command"
              }
            ],
            "simultaneous_options": {
              "key_down_order": "strict",
              "key_up_order": "insensitive",
              "key_up_when": "all",
              "to_after_key_up": [
                {
                  "set_variable": {
                    "name": "fn_locked",
                    "value": 1
                  }
                },
                {
                  "shell_command": "osascript -e 'tell app \"System Events\" to display notification \"Mode fn_locked activated.\" with title \"Keyboard Customizer\" sound name \"Frog\"'"
                }
              ]
            }
          },
          "parameters": {
            "basic.simultaneous_threshold_milliseconds": 500
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "fn_locked",
              "value": 0
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "simultaneous": [
              {
                "key_code": "fn"
              },
              {
                "key_code": "left_control"
              },
              {
                "key_code": "left_option"
              },
              {
                "key_code": "left_command"
              }
            ],
            "simultaneous_options": {
              "key_down_order": "strict",
              "key_up_order": "insensitive",
              "key_up_when": "all",
              "to_after_key_up": [
                {
                  "set_variable": {
                    "name": "fn_locked",
                    "value": 0
                  }
                },
                {
                  "shell_command": "osascript -e 'tell app \"System Events\" to display notification \"Mode fn_locked deactivated.\" with title \"Keyboard Customizer\" sound name \"Frog\"'"
                }
              ]
            }
          },
          "parameters": {
            "basic.simultaneous_threshold_milliseconds": 500
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "fn_locked",
              "value": 1
            }
          ],
          "type": "basic"
        }
      ]
    },
