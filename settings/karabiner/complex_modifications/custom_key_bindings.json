{
  "title": "Complex Modifications Rules",
  "rules": [
    {
      "description": "Disable option+left/right click on browsers",
      "manipulators": [
        {
          "from": {
            "pointing_button": "button1",
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "vk_none"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.google\\.Chrome$",
                "^com\\.apple\\.Safari$",
                "^com\\.apple\\.Firefox$"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "pointing_button": "button2",
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "vk_none"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.google\\.Chrome$",
                "^com\\.apple\\.Safari$",
                "^com\\.apple\\.Firefox$"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "control+left/right click to navigate backward/forward",
      "manipulators": [
        {
          "from": {
            "pointing_button": "button1",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": [
                "shift",
                "command"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "pointing_button": "button2",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": [
                "shift",
                "command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "command+backslash to toggle iTerm.app",
      "manipulators": [
        {
          "from": {
            "key_code": "backslash",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'iTerm'"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.googlecode\\.iterm2$"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "backslash",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "key_code": "h",
              "modifiers": [
                "command"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.googlecode\\.iterm2$"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
