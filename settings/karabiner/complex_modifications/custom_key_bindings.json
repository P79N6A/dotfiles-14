{
  "title": "Complex Modifications Rules",
  "rules": [
    {
      "description": "Toggle fn_locked if hold down Fn key",
      "manipulators": [
        {
          "from": {
            "key_code": "fn"
          },
          "to": [
            {
              "key_code": "fn",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "fn"
            }
          ],
          "to_if_held_down": [
            {
              "set_variable": {
                "name": "fn_locked",
                "value": 1
              }
            },
            {
              "shell_command": "osascript -e 'tell app \"System Events\" to display notification \"Activate fn_locked successfully.\" with title \"Keyboard Customizer\" sound name \"Frog\"'"
            },
            {
              "key_code": "fn",
              "lazy": true
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "fn_locked",
              "value": 0
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 250,
            "basic.to_if_held_down_threshold_milliseconds": 2000
          },
          "type": "basic"
        },
        {
          "from": {
            "key_code": "fn"
          },
          "to": [
            {
              "key_code": "fn",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "fn"
            }
          ],
          "to_if_held_down": [
            {
              "set_variable": {
                "name": "fn_locked",
                "value": 0
              }
            },
            {
              "shell_command": "osascript -e 'tell app \"System Events\" to display notification \"Deactivate fn_locked successfully.\" with title \"Keyboard Customizer\" sound name \"Frog\"'"
            },
            {
              "key_code": "fn",
              "lazy": true
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "fn_locked",
              "value": 1
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 250,
            "basic.to_if_held_down_threshold_milliseconds": 2000
          },
          "type": "basic"
        }
      ]
    },
