{
  "title": "Complex Modifications Rules",
  "rules": [
    {
      "description": "Map ⨍n to left click",
      "manipulators": [
        {
          "from": {
            "key_code": "left_control"
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ],
          "to_if_alone": [
            {
              "pointing_button": "button1"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.google\\.Chrome$",
                "^com\\.apple\\.Safari$",
                "^com\\.apple\\.Firefox$"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "fn"
          },
          "to": [
            {
              "key_code": "fn"
            }
          ],
          "to_if_alone": [
            {
              "pointing_button": "button1"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.google\\.Chrome$",
                "^com\\.apple\\.Safari$",
                "^com\\.apple\\.Firefox$"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "fn",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "fn"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "mission_control"
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 100
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "fn pressed",
              "value": 1
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "fn",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "fn"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "fn"
            },
            {
              "set_variable": {
                "name": "fn pressed",
                "value": 1
              }
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 100
          },
          "to_delayed_action": {
            "to_if_invoked": [
              {
                "set_variable": {
                  "name": "fn pressed",
                  "value": 0
                }
              }
            ],
            "to_if_canceled": [
              {
                "set_variable": {
                  "name": "fn pressed",
                  "value": 0
                }
              }
            ]
          },
          "type": "basic"
        }
      ]
    },
    {
      "description": "Toggle iTerm 1x ⏏ ",
      "manipulators": [
        {
          "from": {
            "key_code": "eject"
          },
          "to": [
            {
              "shell_command": "open -a 'iTerm'"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Disable ⌥ ＋  on browsers",
      "manipulators": [
        {
          "from": {
            "pointing_button": "button1",
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "vk_none"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.google\\.Chrome$",
                "^com\\.apple\\.Safari$",
                "^com\\.apple\\.Firefox$"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "pointing_button": "button2",
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "vk_none"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.google\\.Chrome$",
                "^com\\.apple\\.Safari$",
                "^com\\.apple\\.Firefox$"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Navigate backward/forward when ⌃ ＋  on browser",
      "manipulators": [
        {
          "from": {
            "pointing_button": "button1",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": [
                "command"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "pointing_button": "button2",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": [
                "command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Toggle iTerm ⌘ ＋ ﹨",
      "manipulators": [
        {
          "from": {
            "key_code": "backslash",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'iTerm'"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com\\.googlecode\\.iterm2$"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "backslash",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "key_code": "h",
              "modifiers": [
                "command"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.googlecode\\.iterm2$"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
