{
  "name": "Goldfish Toolkits",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "This extension was created with the awesome goldfishcreativethailand.com",
  "homepage_url": "https://goldfishcreativethailand.com",
  "icons": {
    "16": "icons/nerd.png",
    "32": "icons/nerd.png",
    "48": "icons/nerd.png",
    "128": "icons/nerd.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/background.js",
      "src/tab_shortcuts.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/nerd.png",
    "default_title": "Goldfish Toolkits"
  },
  "commands": {
    "duplicate-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "mac": "MacCtrl+Shift+D"
      },
      "description": "Duplicates the current tab."
    },
    "pin-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "MacCtrl+Shift+P"
      },
      "description": "Pin/Unpin the current tab."
    },
    "move-tab-left": {
      "suggested_key": {
        "default": "Ctrl+Shift+Left",
        "mac": "MacCtrl+Shift+Left"
      },
      "description": "Move current tab to the left."
    },
    "move-tab-right": {
      "suggested_key": {
        "default": "Ctrl+Shift+Right",
        "mac": "MacCtrl+Shift+Right"
      },
      "description": "Move current tab to the right."
    }
  },
  "permissions": [
    "activeTab",
    "contentSettings",
    "contextMenus",
    "declarativeContent",
    "nativeMessaging",
    "notifications",
    "pageCapture",
    "tabCapture",
    "tabs",
    "webNavigation",
    "<all_urls>"
  ]
}
