:verbose: true
:backtrace: true
install:
  --no-ri
  --no-rdoc
  --no-document
  --backtrace
  --verbose
  --quiet
  --install-dir ~/.gem/ruby
  --bindir /usr/local/bin
update:
    --backtrace
    --verbose
    --quiet
    --install-dir ~/.gem/ruby
    --bindir /usr/local/bin
list:
    --install-dir ~/.gem/ruby
    --bindir /usr/local/bin
uninstall:
    --backtrace
    --verbose
    --quiet
    --install-dir ~/.gem/ruby
    --bindir /usr/local/bin
